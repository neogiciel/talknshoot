<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="${relativePackage}.${activityClass}"
    android:background="@drawable/back_nav">

    <LinearLayout
        android:id="@+id/layout1"
        android:orientation="horizontal"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:paddingTop="2dp" >
 
        <TextView
           android:id="@+id/txt_titre"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:paddingLeft="12dp" 
           android:textSize="@dimen/txt_titre_size"
           android:textColor="@color/txt_titre_color"
           android:text="@string/parameterabo_txt_titre" />
  </LinearLayout>
  
  <LinearLayout
       	android:id="@+id/layout2"
       	android:layout_below="@+id/layout1"
		android:orientation="vertical"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:paddingTop="20dp" >
 
      	  <TextView
           android:id="@+id/parameterabo_txt_1"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:paddingLeft="12dp" 
           android:textSize="@dimen/txt_sub_titre_size"
           android:textColor="@color/txt_sub_titre_color"
           android:text="@string/parameterabo_txt_1" />
      
      	  <TextView
           android:id="@+id/parameterabo_txt_2"
           android:layout_below="@+id/parameterabo_txt_1"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:paddingLeft="12dp" 
           android:textSize="@dimen/txt_size"
           android:textColor="@color/txt_color"
           android:text="@string/parameterabo_txt_2" />
      	  
      	  
      	   <TextView
           android:id="@+id/parameterabo_txt_install"
           android:layout_below="@+id/parameterabo_txt_1"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:paddingLeft="12dp" 
           android:textSize="@dimen/txt_size"
           android:textColor="@color/txt_titre_color"
           android:text="waiting..." />
      	  
    
	</LinearLayout>      	  
      	  
   <LinearLayout
       	android:id="@+id/layout3"
       	android:layout_below="@+id/layout2"
		android:orientation="vertical"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:paddingTop="20dp" >
 
      	  <TextView
           android:id="@+id/parameterabo_txt_3"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:paddingLeft="12dp" 
           android:textSize="@dimen/txt_sub_titre_size"
           android:textColor="@color/txt_sub_titre_color"
           android:text="@string/parameterabo_txt_3" />
      
      	  <TextView
           android:id="@+id/parameterabo_txt_4"
           android:layout_below="@+id/parameterabo_txt_4"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:paddingLeft="12dp" 
           android:textSize="@dimen/txt_size"
           android:textColor="@color/txt_color"
           android:text="@string/parameterabo_txt_4" />

      	  <TextView
           android:id="@+id/parameterabo_txt_date"
           android:layout_below="@+id/parameterabo_txt_4"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:paddingLeft="12dp" 
           android:textSize="@dimen/txt_size"
           android:textColor="@color/txt_titre_color"
           android:text="waiting..." />
      	  
      	  <TextView
           android:id="@+id/parameterabo_txt_5"
           android:layout_below="@+id/parameterabo_txt_date"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:paddingLeft="12dp" 
           android:textSize="@dimen/txt_size"
           android:textColor="@color/txt_color"
           android:text="@string/parameterabo_txt_5" />
      	  
      	  
      	  <Button
        	android:id="@+id/btn_parameterabo_premium1"
			android:layout_below="@+id/parameterabo_txt_5"
        	android:layout_width="wrap_content"
        	android:layout_height="wrap_content"
        	android:padding="10dp"
        	android:layout_gravity="center_horizontal"
        	android:textSize="@dimen/txt_button_size"
           	android:textColor="@color/btn_txt_color"
        	android:background="@drawable/android_button"
        	android:text="@string/btn_parameterabo_premium1"/>
      	   	  
    </LinearLayout>    
</RelativeLayout>
